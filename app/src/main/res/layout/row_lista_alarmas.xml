<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="horizontal">

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="27dp"
        android:layout_height="25dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:background="@color/colorAccent"
        app:layout_constraintBottom_toBottomOf="@+id/txtCategoriaRowAlarma"
        app:layout_constraintEnd_toStartOf="@+id/guideline10"
        app:layout_constraintTop_toTopOf="@+id/txtTipoGastoRowAlarma"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/modificaralarma" />

    <TextView
        android:id="@+id/txtCicloRowAlarma"
        android:layout_width="55dp"
        android:layout_height="15dp"
        android:layout_marginEnd="28dp"
        android:layout_marginRight="28dp"
        android:text="15"
        android:textSize="11dp"
        android:textColor="@color/colorAccent"
        app:layout_constraintBottom_toBottomOf="@+id/txtImporteRowAlarma"
        app:layout_constraintEnd_toStartOf="@+id/txtCategoriaRowAlarma"
        app:layout_constraintTop_toTopOf="@+id/txtImporteRowAlarma"
        app:layout_constraintVertical_bias="0.0">

    </TextView>

    <TextView
        android:id="@+id/txtCategoriaRowAlarma"
        android:layout_width="78dp"
        android:layout_height="16dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="40dp"
        android:layout_marginRight="40dp"
        android:text="TextView"
        android:textSize="11dp"
        app:layout_constraintEnd_toStartOf="@+id/imageView2"
        app:layout_constraintTop_toBottomOf="@+id/txtTipoGastoRowAlarma" />

    <TextView
        android:id="@+id/txtTipoGastoRowAlarma"
        android:layout_width="78dp"
        android:layout_height="16dp"
        android:layout_marginTop="12dp"
        android:text="TextView"
        android:textSize="11dp"
        app:layout_constraintEnd_toEndOf="@+id/txtCategoriaRowAlarma"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtImporteRowAlarma"
        android:layout_width="55dp"
        android:layout_height="15dp"
        android:layout_marginStart="36dp"
        android:layout_marginLeft="36dp"
        android:layout_marginTop="24dp"
        android:textColor="@color/colorAccent"
        android:text="euros"
        android:textSize="11dp"
        app:layout_constraintEnd_toStartOf="@+id/txtCicloRowAlarma"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

    </TextView>

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline33"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        app:layout_constraintGuide_begin="64dp" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="369dp" />


</androidx.constraintlayout.widget.ConstraintLayout>